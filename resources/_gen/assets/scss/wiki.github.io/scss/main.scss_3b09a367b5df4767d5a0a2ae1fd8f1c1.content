.enter-up{animation:slideInUp 2s,fadeIn 1s}html,body{height:100%}body{font-family:source sans pro,Arial,sans-serif;font-weight:300;font-size:18px;line-height:1.5;color:#222;background:#fff;position:relative;display:flex;flex-direction:column}a{color:#329998;-webkit-transition:.5s;-o-transition:.5s;transition:.5s}a:hover,a:active{color:#329998;outline:none}::-webkit-selection,::-moz-selection,::selection{background:#a9a9a9}.embedded-video{margin-top:20px;margin-bottom:20px;text-align:center;max-width:90%;min-height:70vh}.slanted-bottom{-webkit-clip-path:polygon(0 0,100% 0,100% 56%,0 100%);clip-path:polygon(0 0,100% 0,100% 96%,0 100%)}nav{-webkit-box-shadow:0 0 10px 0 rgba(0,0,0,.1);-moz-box-shadow:0 0 10px 0 rgba(0,0,0,.1);-ms-box-shadow:0 0 10px 0 rgba(0,0,0,.1);box-shadow:0 0 10px 0 rgba(0,0,0,.1)}.navbar-brand{color:#329998;font-weight:1000}.nav-link{color:#555}.dropdown-menu{margin-top:10px}#scroll-line{position:fixed;z-index:1;height:3.4em;margin-bottom:-2px;background-color:#329998;width:0%}#hero{padding-top:75px;padding-bottom:75px;margin-bottom:50px;color:#fff;background-color:#329998;background-image:-webkit-gradient(linear,0% 0%,100% 100%,color-stop(0,#329998),color-stop(1,#6cc99c));background-image:-webkit-linear-gradient(top left,#329998 0%,#6cc99c 100%);background-image:linear-gradient(to bottom right,#329998 0%,#6cc99c 100%);background-image:-ms-linear-gradient(top left,#329998 0%,#6cc99c 100%)}.hero-divider{width:40%;border-top:3px solid #fff}.hero-logo{width:60%}footer{color:#ccc;background-color:#535353;background-image:-webkit-gradient(linear,0% 0%,100% 100%,color-stop(0,#535353),color-stop(1,#171717));background-image:-webkit-linear-gradient(top left,#535353 0%,#171717 100%);background-image:linear-gradient(to bottom right,#535353 0%,#171717 100%);background-image:-ms-linear-gradient(top left,#535353 0%,#171717 100%);flex-shrink:0;padding-top:1em}